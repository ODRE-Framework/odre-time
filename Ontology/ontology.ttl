                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( odrl:Constraint
                                           odrl:LogicalConstraint
                                         )
                           ] .


###  http://www.w3.org/ns/odrl/2/relation
odrl:relation rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( odrl:Policy
                                          odrl:Rule
                                        )
                          ] ;
              rdfs:range odrl:Asset .


###  http://www.w3.org/ns/odrl/2/remedy
odrl:remedy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf odrl:failure ;
            rdfs:domain odrl:Prohibition ;
            rdfs:range odrl:Duty .


###  http://www.w3.org/ns/odrl/2/rightOperand
odrl:rightOperand rdf:type owl:ObjectProperty ;
                  rdfs:domain odrl:Constraint ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( rdfs:Literal
                                             xsd:anyURI
                                             odrl:RightOperand
                                           )
                             ] .


###  http://www.w3.org/ns/odrl/2/scope
odrl:scope rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/odrl/2/target
odrl:target rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf odrl:relation ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( odrl:Policy
                                        odrl:Rule
                                      )
                        ] ;
            rdfs:range odrl:Asset .


###  http://www.w3.org/ns/odrl/2/trackedParty
odrl:trackedParty rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf odrl:function .


###  http://www.w3.org/ns/odrl/2/trackingParty
odrl:trackingParty rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf odrl:function .


###  http://www.w3.org/ns/odrl/2/undefined
odrl:undefined rdf:type owl:ObjectProperty ;
               rdfs:range odrl:UndefinedTerm .


###  http://www.w3.org/ns/odrl/2/xone
odrl:xone rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf odrl:operand .


